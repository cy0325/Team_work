body {
    background-color: #f2f2f2;
}

.clearfix:before,

.clearfix:after {

    content: " ";

    display: block;

    line-height: 0;

    height: 0;

    font-size: 0;

    visibility: hidden;

}

.clearfix:after {

    clear: both;

}

.fz-primary {
    font-size: 1.2em;
}

.top-header {
    z-index: 999;
    width: 100%;
    background-color: #fff;
    display: table;
    border-bottom: solid 1px rgba(0, 0, 0, 0.2);
    padding: 1% 0;
}

.top-header h3 {
    font-size: 1.6em;
    color: #333;
    margin: 0 30%;
    display: table-cell;
    font-weight: normal;
    text-align: center;
    padding: 0.66em 0;
}

.top-header .text {
    display: inline-block;
    text-align: center;
    width: 15%;
    font-size: 1.2em;
    color: #fc605a;
    border: solid 1px #fc605a;
    border-radius: 5px;
    padding: 1% 0;
    float: right;
    position: absolute;
    right: 3%;
    top: 1.2%;
}

.top-header .texta {
    color: #999;
}

.top-header a {
    display: table-cell;
}

.top-header .icona {
    width: 8%;
    padding-left: 5%;
    vertical-align: middle;
}

