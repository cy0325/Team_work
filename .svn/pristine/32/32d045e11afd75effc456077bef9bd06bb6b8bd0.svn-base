.login {
    width: 100%;
    margin-top: 0.7%;
}

.login form {
    width: 100%;
}

.login form ul {
    width: 100%;
    overflow: hidden;
}

.login form ul li {
    width: 100%;
    padding: 2% 5%;
    background-color: #fff;
    margin-bottom: 1%;
    overflow: hidden;
}

.login form ul li img {
    width: 7%;
    vertical-align: middle;
}

.login form ul li label {
    font-size: 1.2em;
    vertical-align: middle;
    color: #333;
    margin: 0;
}

.login form ul li input {
    width: 60%;
    display: inline-block;
    vertical-align: middle;
    font-size: 1.2em;
    color: #666;
    border: none;
    padding: 0 0 0 2%;
    margin: 0;
}

.login form ul li textarea{
    border: none;
    color: #666;
    background-color: #fff;
    width: 60%;
    display: inline-block;
    font-size: 1.2em;
    padding: 0 0 0 2%;
    vertical-align: middle;
}

.login form ul li input.code {
    width: 65%;
}

.login form ul li input.code+input[type=button] {
    display: inline-block;
    width: 14%;
    padding: 2%;
    color: #fff;
    background-color: #0099ff;
    border-radius: 3px;
    position: absolute;
    margin-top: 0.5%;
    right: 3%;
}

.login form input[type="submit"],
.gobuy-btn {
    width: 94%;
    margin: 5% 3%;
    display: block;
    padding: 4% 0;
    background-color: #00cc66;
    color: #fff;
    text-align: center;
    font-size: 1.2em;
    border-radius: 0.3em;
    border: none;
}

.buybanner img {
    width: 100%;
}

