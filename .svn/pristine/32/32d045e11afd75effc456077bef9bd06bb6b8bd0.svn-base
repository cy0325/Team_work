.page-header {
    width: 100%;
    background-color: #00cc66;
    overflow: hidden;
    position: fixed;
    top: 0;
    z-index: 999;
}

.page-header .back {
    position: absolute;
    left: 0;
    top: 25%;
    width: 8%;
    padding-left: 5%;
    vertical-align: middle;
    z-index: 999;
}

.page-header .back img {
    display: block;
    width: 100%;
    height: auto;
}

.page-header h3 {
    position: relative;
    font-size: 1.6em;
    color: #fff;
    font-weight: normal;
    text-align: center;
    padding: 0.55em 0;
}

.page-header input {
    width: 60%;
    border: none;
    border-radius: 3em;
    margin: 2% 5%;
    padding: 0.5% 5%;
    font-size: 1.2em;
    float: left;
    height: 32px;
}

.page-header span {
    width: 15%;
    display: inline-block;
    margin: 0;
    float: right;
    height: 100%;
    /* padding: 2.5% 0; */
}

.page-header span img {
    width: 50%;
    display: block;
}

.container {
    width: 100%;
    padding-top: 0.6%;
}

.container .error {
    background-color: #fff;
    width: 100%;
    margin-top: 0;
}

.container .error .img {
    display: block;
    width: 100%;
}

.container .error .img img {
    display: block;
    width: 100%;
    height: auto;
}

.container .error .back-btn {
    width: 100%;
    padding: 10% 19%;
}

.container .error .back-btn a {
    display: block;
    width: 100%;
    color: #fff;
    border: none;
    padding: 3% 0;
    font-size: 1.2em;
    border-radius: 0.3em;
    background-color: #00cc66;
    text-align: center;
}

.container p.indent {
    text-indent: 2em;
}

.container .ban li {
    width: 100%;
    height: 18em;
}

.container .ban a {
    width: 100%;
    display: block;
    overflow: hidden;
}

.container .ban img {
    width: 100%;
    min-height: 18em;
    display: block;
}

.container .shop {
    width: 100%;
    margin-top: 5%;
}

.container .shop h3 {
    width: 90%;
    font-size: 1.4875em;
    padding: 3% 5%;
    background-color: #fff;
    overflow: hidden;
}

.container .shop h3 span {
    float: right;
    width: 4%;
    display: block;
}

.container .shop h3 span img {
    width: 100%;
    display: block;
}

.container .shop h3 a {
    width: 100%;
    color: #45c018;
    display: block;
}

.container .shop dl {
    margin: 2% 0;
    width: 100%;
    overflow: hidden;
}

.container .shop dl dd {
    width: 44%;
    min-height: 11em;
    max-height: 15em;
    overflow: hidden;
    margin: 0 3%;
    float: left;
    position: relative;
}

.container .shop dl dd a {
    width: 100%;
    display: block;
    height: 100%;
}

.container .shop dl dd img {
    width: 100%;
    height: 100%;
    display: block;
}

.container .shop dl dd b {
    position: absolute;
    bottom: 0;
    left: 0;
    display: block;
    width: 80%;
    padding: 5% 10%;
    color: #fff;
    font-weight: normal;
    font-size: 1.16em;
